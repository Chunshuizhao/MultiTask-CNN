layer {
  name: "batches_train"
  bottom: "pool4"
  bottom: "label_pos"
  top: "pool4_batch1"
  top: "pool4_batch2"
  top: "pool4_batch3"
  type: "BatchSplit"
  include {
    phase: TRAIN
  }
  batch_split_param {
    emptyfill: 0
    backward: true
    split_threshold: 0
    split_threshold: 1
  }
}

layer {
  name: "labels_train"
